<p id="notice"><%= notice %></p>

<div class="container">
  <div class="row">
    <div class="col-lg-8 col-md-10 mx-auto">
      <table>
        <thead>
          <tr>
            <th>Twiclones2とは</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
            <tr>
              <td>Twitterのクローンアプリです。
              <br>つぶやきの投稿と、編集ができます。
              <br></td>
            </tr>
        </tbody>
      </table>
      <br>
      <table>
        <thead>
          <tr>
            <th>投稿一覧</th>
            <th colspan="3"></th>
          </tr>
        </thead>
        <tbody>
          <% @clones.each do |clone| %>
            <tr>
              <td><%= clone.content %></td>
              <td><%= link_to '　編集　', edit_clone_path(clone) %></td>
              <td><%= link_to '　削除　', clone, method: :delete, data: { confirm: '削除しますか?' } %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    <br>
    <%= link_to '新規投稿', new_clone_path %>
   </div>
  </div>
</div>
  
<br>


